[mysqld]
wsrep_provider=/usr/lib/libgalera_smm.so
wsrep_cluster_address=gcomm://192.168.200.10,192.168.200.11,192.168.200.12
wsrep_sst_method=rsync
wsrep_cluster_name=galera_cluster
